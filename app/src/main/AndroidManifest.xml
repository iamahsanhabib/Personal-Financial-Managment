<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.registersignin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_name"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RegisterSignIn">
        <activity android:name=".ExpenseList"></activity>
        <activity android:name=".IncomeList" />
        <activity android:name=".Add_Expense" />
        <activity android:name=".Income" />
        <activity android:name=".Login" />
        <activity android:name=".Registration" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>